    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo test",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib"
                ]
            },
            "args": []
        },
        


        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'mayara-server'",
            "cargo": {
                "args": [
                    "build",
                    
                ],
                "filter": {
                    "name": "mayara-server",
                    "kind": "bin"
                }
            },
            "args": [ 
                    "-v",  "--allow-wifi"
               ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "attach",
            "name": "Attach to running 'mayara'",
            "program": "${workspaceFolder}/target/mayara-server",
        },
        
    ]
}
